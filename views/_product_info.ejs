<div class="tab-pane fade <% if(typeof(pane) !== 'undefined' && (pane == 'ci' || pane =='ad' || pane == 'em' || pane == 'pn')) { %>show active<% } %>"
    id="contact-pane" role="tabpanel" aria-labelledby="contact-tab">
    <ul class="nav nav-tabs nav-fill flex-column flex-sm-row" id="contactTabs" role="tablist">
        <li class="nav-item">
            <a class="nav-link <% if(typeof(pane) !== 'undefined' && pane == 'ci') { %>active<% } %>" id="primary-tab"
                data-toggle="tab" href="#primaryTab" role="tab" aria-controls="home"
                aria-selected="true"><%- include('assets/symbols/contact.svg') %>All Products</a>
        </li>
        <li class="nav-item">
            <a class="nav-link <% if(typeof(pane) !== 'undefined' && pane == 'ad') { %>active<% } %>" id="addresses-tab"
                data-toggle="tab" href="#addressesTab" role="tab" aria-controls="contact"
                aria-selected="false"><%- include('assets/symbols/location.svg') %>New Product</a>
        </li>
        <li class="nav-item <% if(typeof(pane) !== 'undefined' && pane == 'em') { %>active<% } %>">
            <a class="nav-link" id="emails-tab" data-toggle="tab" href="#emailsTab" role="tab" aria-controls="profile"
                aria-selected="false"><%- include('assets/symbols/mail.svg') %>Archived</a>
        </li>
        <!-- <li class="nav-item">
            <a class="nav-link <% if(typeof(pane) !== 'undefined' && pane == 'pn') { %>active<% } %>" id="phone-tab"
                data-toggle="tab" href="#phoneTab" role="tab" aria-controls="contact"
                aria-selected="false"><%- include('assets/symbols/phone.svg') %>Phone Numbers</a>
        </li> -->
    </ul>
    <div class="tab-content" id="contactTabContent">
        <%- include('./_product_all.ejs') %>
        <%- include('./_product_new.ejs') %>
        <%- include('./_product_archived.ejs') %>
        
    </div>
</div>
