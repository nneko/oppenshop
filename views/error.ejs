<main class="container main-center-section">
    <section class="error-msg">
        <h1 class="error-message"><b><% if(typeof(error) !== 'undefined') {%><%= error.message %><% } %></b></h1>
        <p class="error-code"><% if(typeof(error) !== 'undefined') {%><%= error.status %><% } %></p>
    <% if(error.code == 'EBADCSRFTOKEN') { %>
        <p>Invalid form token.</p>
    <% } %>
    </section>
</main>