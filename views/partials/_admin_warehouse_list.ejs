                                            <% for(let k=0;k<warehouses[i].images.length;k++) { %>
                                            <img class="storefront-image" alt="<%= warehouses[i].images[k].originalname %>" src="<%= warehouses[i].images[k].src %>" />
                                            <% } %>
                                            <% } else { %>
                                            <figure class="storefront-image">
                                                <%- include('../assets/symbols/shop.svg') %>
                                            </figure>
                                            <% } %>
                                        </div>
                                        <div class="item-card-right extra-info">
                                                <% if(typeof(warehouses[i].status !== 'undefined')) { %>
                                                    <h4>Status</h4>
                                                    <p>
                                                        <% if(warehouses[i].status == 'active') { %>
                                                        Open
                                                        <% } else { %>
