<div class="tab-pane fade <% if(typeof(pane) !== 'undefined' && pane == 'freight') { %>show active<% } %>" id="freight-pane"
    role="tabpanel" aria-labelledby="freight-tab">
    <h2 class="pane-h2">Freight Management</h2>
    <ul class="nav nav-tabs nav-fill flex-column flex-sm-row" id="freightTabs" role="tablist">
        <li class="nav-item">
            <a class="nav-link <% if(typeof(pane) !== 'undefined' && pane != 'track') { %>active<% } %>" id="freight-alert-tab"
                data-toggle="tab" href="#freight-alert-pane" role="tab" aria-controls="new" aria-selected="false">
                <%- include('../assets/symbols/notification.svg') %>Alert
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link <% if(typeof(pane) !== 'undefined' && pane == 'track') { %>active<% } %>"
                id="freight-track-tab" data-toggle="tab" href="#freight-track-pane" role="tab" aria-controls="all"
                aria-selected="true">
                <%- include('../assets/symbols/location.svg') %>Track
            </a>
        </li>
    </ul>
    <div class="tab-content" id="freightTabContent">
        <%- include('./_freight_alert.ejs') %>
        <%- include('./_freight_track.ejs') %>
    </div>
</div>