<div class="tab-pane fade <% if(typeof(pane) !== 'undefined' && (pane == 'pd' || pane =='pd-all' || pane == 'pd-new' || pane == 'pd-arc')) { %>show active<% } %>" id="products-pane" role="tabpanel" aria-labelledby="products-tab">
    <ul class="nav nav-tabs nav-fill flex-column flex-sm-row" id="productsTabs" role="tablist">
        <li class="nav-item">
            <a class="nav-link <% if(typeof(pane) !== 'undefined' && pane == 'cl' || pane == 'sf') { %>active<% } %>"
                id="catalog-products-tab" data-toggle="tab" href="#catalogs-pane" role="tab" aria-controls="all"
                aria-selected="true"><%- include('assets/symbols/library.svg') %>Catalog</a>
        </li>
        <li class="nav-item">
            <a class="nav-link <% if(typeof(pane) !== 'undefined' && pane == 'in') { %>active<% } %>" id="product-inventory-tab"
                data-toggle="tab" href="#inventory-pane" role="tab" aria-controls="all"
                aria-selected="true"><%- include('assets/symbols/gallery.svg') %>Shop Inventory</a>
        </li>
        <li class="nav-item">
            <a class="nav-link <% if(typeof(pane) !== 'undefined' && pane == 'pd-new') { %>active<% } %>" id="new-products-tab"
                data-toggle="tab" href="#newProductsTab" role="tab" aria-controls="new"
                aria-selected="false"><%- include('assets/symbols/add.svg') %>New Product</a>
        </li>
        <li class="nav-item <% if(typeof(pane) !== 'undefined' && pane == 'pd-arc') { %>active<% } %>">
            <a class="nav-link" id="withdrawn-product-tab" data-toggle="tab" href="#withdrawn-product-pane" role="tab" aria-controls="archive"
                aria-selected="false"><%- include('assets/symbols/box.svg') %>Withdrawn</a>
        </li>
    </ul>
    <div class="tab-content" id="productsTabContent">
        <%- include('./_product_catalog.ejs') %>
        <%- include('./_product_all.ejs') %>
        <%- include('./_product_new.ejs') %>
        <%- include('./_product_withdrawn.ejs') %>
    </div>
</div>
