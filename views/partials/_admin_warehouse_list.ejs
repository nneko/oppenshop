                        <ul>
                            <li>
                                <span>
                                    <p class="item-card-label">
                                        <b><% if(typeof(warehouses[i].displayName) !== 'undefined') {%><%= warehouses[i].displayName %><% } else { %><%= warehouses[i].name %><% } %></b>
